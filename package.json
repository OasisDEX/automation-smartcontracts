{
  "name": "automation-smartcontracts",
  "version": "1.0.0",
  "description": "Production Smart Contracts for automation (including stop loss)",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test",
    "test:aggregator": "npx hardhat test test/automation-aggregator.ts",
    "test:cmv": "npx hardhat test test/constant-multiple-validator.ts",
    "test:atp": "npx hardhat test test/auto-take-profit.ts",
    "test:sr": "npx hardhat test test/service-registry.ts",
    "build": "npx hardhat compile",
    "clean": "rm -rf artifacts cache typechain",
    "lint:fix": "./node_modules/.bin/solhint --fix -f table contracts/*.sol contracts/**/*.sol && ./node_modules/.bin/eslint --fix test/**/*.ts scripts/**/*.ts",
    "lint:contracts": "./node_modules/.bin/solhint -f table contracts/*.sol contracts/**/*.sol",
    "lint:typescript": "eslint test/**/*.ts scripts/**/*.ts",
    "lint": "yarn lint:contracts && yarn lint:typescript",
    "format:fix": "prettier --write --plugin=prettier-plugin-solidity contracts/*.sol contracts/**/*.sol test/**/*.ts scripts/**/*.ts",
    "format": "prettier --check --plugin=prettier-plugin-solidity contracts/*.sol contracts/**/*.sol test/**/*.ts"
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OasisDEX/automation-smartcontracts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OasisDEX/automation-smartcontracts/issues"
  },
  "homepage": "https://github.com/OasisDEX/automation-smartcontracts#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomicfoundation/hardhat-toolbox": "2.0.2",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@oasisdex/addresses": "^0.1.13",
    "@oasisdex/dma-library": "^0.5.18",
    "@oasisdex/multiply": "^0.2.10",
    "@tenderly/hardhat-tenderly": "^1.1.6",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/hardhat": "7",
    "@types/chai": "^4.2.22",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@types/ramda": "^0.27.64",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "axios": "^0.26.1",
    "bignumber.js": "9.0.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^5.1.1",
    "ethers": "^5.5.1",
    "fs": "0.0.1-security",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-tenderly": "^1.0.1",
    "hardhat-tracer": "^2.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.2.0",
    "ramda": "^0.27.1",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "ts-try": "^0.2.0",
    "typechain": "^8.3.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@oasisdex/automation": "^1.5.9-alpha0",
    "@openzeppelin/contracts": "^4.8.3",
    "@uniswap/swap-router-contracts": "1.3.0",
    "@uniswap/v3-core": "1.0.2-solc-0.8-simulate",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2"
  }
}
